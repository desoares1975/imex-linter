{
  "temp-directory": "./coverage/.nyc_output",
  "include": ["server/**/*.js"],
  "exclude": [
    "**/node_modules/**",
    "**/test/**",
    "**/coverage/**"
  ],
  "check-coverage": true,
  "all": true,
  "lines": 50,
  "statements": 50,
  "functions": 50,
  "branches": 50,
  "reporter": [
    "text",
    "text-summary",
    "lcov"
  ],
  "sourceMap": false
}
